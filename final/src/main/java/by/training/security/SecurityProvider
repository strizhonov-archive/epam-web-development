

@Bean
public class SecurityProviderImpl implements SecurityProvider {

    private EnumMap<AccessForType, SecuritySupervisor> registered;

    public ActionCommandProviderImpl() {
        this.registered = new EnumMap<>(AccessForType.class);
    }

    @Override
    public AccessForType get(ActionCommand command) {
        AccessForType securityType = command.getType.getSecurityType();
        return registired.get(securityType);
    }

    @Override
    public void register(SecuritySupervisor... supervisors) {
        for (SecuritySupervisor supervisor : supervisors) {
            registered.put(supervisor.getType(), supervisor);
        }
        
    }

}